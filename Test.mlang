PRINT "\n\n--== PRINT EXAMPLE ==--\n\n"

PRINT "Hello World!"

PRINT "\n\n--== VAR EXAMPLE ==--\n\n"

// integer
LET var: 100

// still an integer
var: 200

// double
//    / - (notice the asterisk)
var: *1.5

PRINT "var is equal to " var

PRINT "\n\n--== IF EXAMPLE ==--\n\n"

//             / - (line of the ENDIF statement)
IF 100 < 200 240
    PRINT "Obviously"
ENDIF

PRINT "\n\n--== LOOP EXAMPLE ==--\n\n"

LET i: 0

IF i < 11 340
    PRINT "i: " i "\n"
    i + 1
    GOTO 300
ENDIF

PRINT "\n--== ARRAY EXAMPLE ==--\n\n"

ARR a: 10 20 30 40 50

LET o: 0

IF o < 5 460
    PRINT a o " - a [" o "]\n"
    o + 1
    GOTO 420
ENDIF

PRINT "\n--== CHANGE-IN-ARRAY EXAMPLE ==--\n\n"

a 4: 60

o: 0

IF o < 5 580
    PRINT a o " - a [" o "]\n"
    o + 1
    GOTO 540
ENDIF

END 0